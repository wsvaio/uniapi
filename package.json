{
  "name": "@wsvaio/uniapi",
  "version": "1.0.0",
  "description": "一个让你轻松使用 uni.request 的封装库，支持前后置中间件、错误拦截、最终（finally）中间件、派生配置、柯里化配置等",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/uniapi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/uniapi.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/uniapi/issues"
  },
  "keywords": [
    "wsvaio",
    "uniapp",
    "request",
    "http",
    "api"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "uni",
    "build": "tsup",
    "commit": "czg",
    "prerelease": "npm run build && npm version prerelease && npm publish",
    "prepatch": "npm run build && npm version prepatch && npm publish",
    "patch": "npm run build && npm version patch && npm publish",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@wsvaio/utils": "^0.0.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@dcloudio/uni-app": "3.0.0-alpha-3070720230316001",
    "@dcloudio/uni-components": "3.0.0-alpha-3070720230316001",
    "@dcloudio/uni-h5": "3.0.0-alpha-3070720230316001",
    "vue": "^3.2.45",
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-automator": "3.0.0-alpha-3070720230316001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3070720230316001",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-3070720230316001",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3070720230316001",
    "typescript": "^4.9.4",
    "vite": "4.0.4",
    "vue-tsc": "^1.0.24",
    "@types/node": "^18.15.3",
    "@vue/tsconfig": "^0.1.3",
    "@wsvaio/commitlint-config": "^0.0.4",
    "@wsvaio/eslint-config": "^0.0.4",
    "czg": "^1.6.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "tsup": "^6.6.3"
  }
}
