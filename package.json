{
  "name": "@wsvaio/uniapi",
  "version": "1.0.5-0",
  "description": "一个让你轻松使用 uni.request 的封装库，支持前后置中间件、错误拦截、最终（finally）中间件、派生配置、柯里化配置等",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/uniapi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/uniapi.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/uniapi/issues"
  },
  "keywords": [
    "wsvaio",
    "uniapp",
    "request",
    "http",
    "api"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "uni",
    "commit": "czg",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@wsvaio/utils": "^1.0.12"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@dcloudio/types": "^3.3.3",
    "@dcloudio/uni-app": "3.0.0-alpha-3080520230612002",
    "@dcloudio/uni-automator": "3.0.0-alpha-3080520230612002",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3080520230612002",
    "@dcloudio/uni-components": "3.0.0-alpha-3080520230612002",
    "@dcloudio/uni-h5": "v3.0.0-alpha-3080520230612002",
    "@dcloudio/uni-stacktracey": "v3.0.0-alpha-3080520230612002",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3080520230612002",
    "@types/node": "^20.3.1",
    "@vue/tsconfig": "^0.4.0",
    "@wsvaio/commitlint-config": "^0.0.6",
    "@wsvaio/eslint-config": "^0.0.6",
    "czg": "^1.6.1",
    "eslint": "^8.42.0",
    "simple-git-hooks": "^2.8.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "4.3.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  }
}
